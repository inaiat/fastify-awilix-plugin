{
    "name": "fastify-awilix-plugin",
    "version": "0.0.1",
    "description": "Simple, unobtrusive depency injection for fastify.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
      "build": "rimraf ./dist && mkdir dist && tsc --outDir dist && git rev-parse HEAD > BUILD_SHA",
      "lint": "eslint \"*/**/*.{js,ts,tsx}\"",
      "lint:fix": "prettier --loglevel warn --write \"src/**/*.{ts,tsx}\" && eslint \"*/**/*.{js,ts,tsx}\" --quiet --fix",
      "prepublishOnly": "npm run build",
      "test": "npm run build && npm run test:unit && npm run lint",
      "test:unit": "jest"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/inaiat/fastify-awilix-plugin"
    },
    "keywords": [
      "fastify",
      "depency injection"
    ],
    "contributors": [
      "Inaiat <inaiat@gmail.com>"
    ],
    "license": "MIT",
    "licenses": [
      {
        "type": "MIT",
        "url": "http://opensource.org/licenses/MIT"
      }
    ],
    "bugs": {
      "url": "https://github.com/inaiat/fastify-awilix-plugin/issues"
    },
    "homepage": "https://github.com/inaiat/fastify-awilix-plugin",
    "engines": {
      "node": ">= 14.0.0"
    },
    "dependencies": {
      "fastify-plugin": "^3.0.1"
    },
    "peerDependencies": {
      "fastify": "^3.28.0"
    },
    "devDependencies": {
      "@types/jest": "27.4.1",
      "@types/node": "17.0.25",
      "@types/semver": "7.3.9",
      "@typescript-eslint/eslint-plugin": "5.20.0",
      "@typescript-eslint/parser": "5.20.0",
      "eslint": "8.14.0",
      "eslint-config-prettier": "8.5.0",
      "eslint-plugin-prettier": "4.0.0",
      "fastify": "^3.28.0",
      "got": "12.0.3",
      "jest": "27.5.1",
      "prettier": "2.6.2",
      "rimraf": "3.0.2",
      "semver": "7.3.7",
      "ts-jest": "27.1.4",
      "typescript": "4.6.3"
    },
    "files": [
      "dist"
    ]
  }
  